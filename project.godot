[application]

config/name="Refactal test2"
config/description="Serious game to teach good programming habits"
run/main_scene="res://main/Main.tscn"
run/flush_stdout_on_print=true
boot_splash/image="res://assets/textures/splash.png"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"

[autoload]

Vars="*res://globals/Vars.gd"
Items="*res://globals/Items.gd"
Settings="*res://globals/settings/Settings.tscn"
Server="res://globals/parser/ParserClient.tscn"
Constraints="*res://globals/constaints/Constraints.tscn"
Progress="*res://globals/progress/Progress.gd"
SilentWolf="*res://addons/silent_wolf/SilentWolf.gd"
ActionsData="*res://globals/PlayerData/ActionsData.gd"
DialogicClass="*res://addons/dialogic/Other/DialogicClass.gd"

[display]

window/size/width=1920
window/size/height=1080
window/size/fullscreen=true
window/handheld/orientation="sensor"
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PoolStringArray( "res://addons/dialogic/plugin.cfg" )

[gui]

theme/custom_font="res://assets/fonts/default.tres"

[importer_defaults]

ogg_vorbis={
"loop": false,
"loop_offset": 0
}

